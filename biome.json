{
	"$schema": "https://biomejs.dev/schemas/2.3.10/schema.json",
	"javascript": {
		"formatter": {
			"quoteStyle": "double"
		},
		"parser": {
			"unsafeParameterDecoratorsEnabled": true
		}
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true,
			"nursery": {
				"useImportType": "off"
			}
		}
	}
}
